// Remove a specific Element from an arrab ( no predefined functions allowed)
        function removeelement(arrab1,n) 
{
    var w=arrab1.length;
    for (a=0; a<w;a++)
    {
      if (arrab1[a]==n)
          { for (var b=a;b<w-1;b++)
            {
                arrab1[b]=arrab1[b+1] ;
            } 
            w=w-1;
            return;
          }
     }

}
var arrab1=[1,1,1,3,4,8,4];
console.log(removeelement(arrab1,8)); 
console.log
